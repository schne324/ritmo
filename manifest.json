{
  "manifest_version": 2,
  "name": "Ritmo",
  "description": "Change tempo of audio and video content",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["<all_urls>"]
  }],
  "permissions": [
    "activeTab"
  ]
}
